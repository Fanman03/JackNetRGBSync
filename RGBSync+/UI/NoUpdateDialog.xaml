<Window x:Class="SyncStudio.WPF.UI.NoUpdateDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:p = "clr-namespace:SyncStudio.WPF.Properties"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SyncStudio.WPF.UI"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="{Binding ConverterParameter=Dialog.NoUpdateTitle, Converter={StaticResource LocConverter}}" Height="185" Width="405" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen" Topmost="True">
    <Grid Background="#525252">
        <Button Content="{Binding ConverterParameter=Dialog.OKCommand, Converter={StaticResource LocConverter}}" HorizontalAlignment="Left" Margin="307,117,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" />
        <TextBlock Name="Message" Text="{Binding ConverterParameter=Dialog.NoUpdateText, Converter={StaticResource LocConverter}}" TextWrapping="WrapWithOverflow" FontSize="18" Foreground="White"  HorizontalAlignment="Left" Margin="123,24,0,0" VerticalAlignment="Top" Width="259">
        </TextBlock>
        <Image Margin="10,10,293,51" Source="error.png" Stretch="Fill"/>
    </Grid>
</Window>
