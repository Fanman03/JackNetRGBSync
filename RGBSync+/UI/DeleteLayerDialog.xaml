<Window x:Class="RGBSyncPlus.UI.DeleteLayerDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:p = "clr-namespace:RGBSyncPlus.Properties"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RGBSyncPlus.UI"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Remove LED group?" Height="185" Width="405" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen" Topmost="True">
    <Grid Background="#525252">
        <Button Click="YesBtnClick" Content="{x:Static p:Resources.Dialog_Yes}" HorizontalAlignment="Left" Margin="227,117,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Click="NoBtnClick" Content="{x:Static p:Resources.Dialog_No}" HorizontalAlignment="Left" Margin="307,117,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock TextWrapping="WrapWithOverflow" FontSize="18" Foreground="White"  HorizontalAlignment="Left" Margin="130,28,0,0" VerticalAlignment="Top" Width="259">
            <Run Foreground="LightGray" Text="Remove LED group?"/><Run Name="GroupName"/><Run Foreground="LightGray" Text="?"/>
        </TextBlock>
        <Image Margin="10,10,293,51" Source="icons8-delete-view-96.png" Stretch="Fill"/>
    </Grid>
</Window>
